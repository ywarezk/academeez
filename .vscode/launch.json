{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "./node_modules/.bin/astro dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "command": "./node_modules/.bin/astro build",
      "name": "Build",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "test",
      "runtimeExecutable": "pnpm",
      "args": ["playwright", "test"],
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
